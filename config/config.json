{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "bbhc-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/bbhc/BbhcWebPart.js",
          "manifest": "./src/webparts/bbhc/BbhcWebPart.manifest.json"
        }
      ]
    },
    "provider-documents-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/providerDocuments/ProviderDocumentsWebPart.js",
          "manifest": "./src/webparts/providerDocuments/ProviderDocumentsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "BbhcWebPartStrings": "lib/webparts/bbhc/loc/{locale}.js",
    "ProviderDocumentsWebPartStrings": "lib/webparts/providerDocuments/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}